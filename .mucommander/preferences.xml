<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<preferences version="0.9.2">
    <display_compact_file_size>true</display_compact_file_size>
    <time_format>HH:mm</time_format>
    <lookAndFeel>javax.swing.plaf.metal.MetalLookAndFeel</lookAndFeel>
    <quit_confirmation>false</quit_confirmation>
    <show_splash_screen>false</show_splash_screen>
    <preferred_encodings>UTF-8,UTF-16,ISO-8859-1,windows-1252,KOI8-R,Big5,GB18030,EUC-KR,Shift_JIS,ISO-2022-JP,EUC-JP</preferred_encodings>
    <check_for_updates_on_startup>true</check_for_updates_on_startup>
    <language>en</language>
    <date_format>dd/MM/yyyy</date_format>
    <enable_system_notifications>false</enable_system_notifications>
    <date_separator>/</date_separator>
    <auto_refresh>
        <wait_after_refresh>10000</wait_after_refresh>
        <check_period>3000</check_period>
    </auto_refresh>
    <ftp>
        <list_hidden_files>false</list_hidden_files>
    </ftp>
    <mail>
        <smtp_port>25</smtp_port>
    </mail>
    <log>
        <level>WARNING</level>
        <buffer_size>200</buffer_size>
    </log>
    <smb>
        <use_extended_security>false</use_extended_security>
        <lm_compatibility>0</lm_compatibility>
    </smb>
    <status_bar>
        <visible>true</visible>
    </status_bar>
    <bonjour>
        <discovery_enabled>true</discovery_enabled>
    </bonjour>
    <toolbar>
        <icon_scale>1.0</icon_scale>
        <visible>true</visible>
    </toolbar>
    <command_bar>
        <icon_scale>1.0</icon_scale>
        <visible>false</visible>
    </command_bar>
    <shell>
        <history_size>100</history_size>
        <use_custom>false</use_custom>
        <autodect_encoding>true</autodect_encoding>
    </shell>
    <startup_folder>
        <on_startup>lastFolders</on_startup>
        <custom_folders>
            <left>/root</left>
            <right>/root</right>
        </custom_folders>
    </startup_folder>
    <theme>
        <path>Native</path>
        <type>predefined</type>
    </theme>
    <file_table>
        <icon_scale>1.0</icon_scale>
        <show_single_tab_header>false</show_single_tab_header>
        <cd_follows_symlinks>false</cd_follows_symlinks>
        <show_system_folders>true</show_system_folders>
        <use_system_file_icons>applications</use_system_file_icons>
        <show_hidden_files>true</show_hidden_files>
        <show_ds_store_files>true</show_ds_store_files>
        <auto_size_columns>true</auto_size_columns>
        <show_folders_first>true</show_folders_first>
    </file_table>
</preferences>
